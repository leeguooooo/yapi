{
  "name": "yapi-vendor",
  "version": "2.0.0",
  "description": "YAPI",
  "main": "server/app.js",
  "scripts": {
    "dev-copy-icon": "cp -r static/iconfont ./",
    "dev-server": "nodemon server/app.js dev -L",
    "install-server": "node server/install.js",
    "dev-client": "NODE_OPTIONS=\"--require ./tools/force-sass-silent.js\" SASS_SILENCE_DEPRECATION=all SASS_SILENCE_DEPRECATIONS=all pnpm run dev-copy-icon && NODE_OPTIONS=\"--require ./tools/force-sass-silent.js\" SASS_SILENCE_DEPRECATION=all SASS_SILENCE_DEPRECATIONS=all vite --config vite.config.js",
    "dev": "NODE_OPTIONS=\"--require ./tools/force-sass-silent.js\" SASS_SILENCE_DEPRECATION=all SASS_SILENCE_DEPRECATIONS=all concurrently \"pnpm run dev-server\" \"pnpm run dev-client\"",
    "start": "node server/app.js",
    "test": "ava",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "build-client": "NODE_OPTIONS=\"--require ./tools/force-sass-silent.js\" SASS_SILENCE_DEPRECATION=all SASS_SILENCE_DEPRECATIONS=all vite build --config vite.config.js",
    "npm-publish": "node ./npm-publish.js",
    "docs": "ydoc build"
  },
  "scripts-info": {
    "start": "运行生产环境服务器",
    "install-server": "初始化数据库数据，用于安装",
    "dev": "运行开发服务器",
    "dev-server": "运行后端开发服务器",
    "dev-client": "运行前端开发服务器",
    "test": "执行测试"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:leeguooooo/yapi.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@apidevtools/json-schema-ref-parser": "^15.1.3",
    "@koa/multer": "^4.0.0",
    "@koa/router": "^14.0.0",
    "@readme/openapi-parser": "^5.2.1",
    "@toast-ui/editor": "^3.2.2",
