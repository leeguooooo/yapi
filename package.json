    "extend": "^3.0.2",
    "fs-extra": "^11.3.2",
    "generate-schema": "^2.6.0",
    "immer": "^11.0.1",
    "js-base64": "^3.7.8",
    "json-schema-faker": "^0.5.9",
    "json5": "^2.2.3",
    "jsondiffpatch": "0.4.1",
    "jsonwebtoken": "^9.0.2",
    "jsrsasign": "^11.1.0",
    "koa": "^3.1.1",
