{
  "name": "yapi-vendor",
  "version": "1.11.0",
  "description": "YAPI",
  "main": "server/app.js",
  "scripts": {
    "dev-copy-icon": "cp -r static/iconfont ./",
    "dev-server": "nodemon server/app.js dev -L",
    "install-server": "node server/install.js",
    "dev-client": "pnpm run dev-copy-icon && vite --config vite.config.js",
    "dev": "concurrently \"pnpm run dev-server\" \"pnpm run dev-client\"",
    "start": "node server/app.js",
    "test": "ava",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "build-client": "vite build --config vite.config.js",
    "npm-publish": "node ./npm-publish.js",
    "docs": "ydoc build"
  },
  "scripts-info": {
    "start": "运行生产环境服务器",
    "install-server": "初始化数据库数据，用于安装",
    "dev": "运行开发服务器",
    "dev-server": "运行后端开发服务器",
    "dev-client": "运行前端开发服务器",
    "test": "执行测试"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:YMFE/yapi.git"
  },
  "author": "",
