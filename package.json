    "@vitejs/plugin-react": "^5.1.1",
    "antd": "^6.0.0",
    "ava": "^6.4.1",
    "brace": "^0.11.1",
    "concurrently": "^9.2.1",
    "conventional-changelog-cli": "^5.0.0",
    "core-decorators": "^0.20.0",
