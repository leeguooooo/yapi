    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "ghooks": "^2.0.4",
    "less": "^4.4.2",
    "markdown-it-include": "^2.0.0",
    "nodemon": "^3.1.11",
